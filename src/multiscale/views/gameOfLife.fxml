<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.Pane?>

<Pane prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="multiscale.controllers.gameOfLife.GameOfLifeController">
    <children>
        <GridPane fx:id="drawGridArea" layoutX="193.0" layoutY="13.0" onMouseClicked="#handleMouseClicked" prefHeight="770.0" prefWidth="1000.0" />
        <Button fx:id="drawButton" layoutX="30.0" layoutY="166.0" mnemonicParsing="false" onAction="#drawGrid" prefHeight="27.0" prefWidth="80.0" text="Rysuj" />
        <Button fx:id="startSimulationButton" layoutX="30.0" layoutY="201.0" mnemonicParsing="false" onAction="#start" prefHeight="27.0" prefWidth="80.0" text="Start" />
        <Button fx:id="wipeResultsButton" layoutX="30.0" layoutY="236.0" mnemonicParsing="false" onAction="#wipeData" prefHeight="27.0" prefWidth="80.0" text="Wyczyść" />
        <Label layoutX="11.0" layoutY="27.0" text="Wysokość" />
        <Label layoutX="78.0" layoutY="27.0" text="Szerokość" />
        <TextField fx:id="gridHeightField" layoutX="22.0" layoutY="53.0" prefHeight="27.0" prefWidth="42.0" />
        <TextField fx:id="gridWidthField" layoutX="89.0" layoutY="53.0" prefHeight="27.0" prefWidth="42.0" />
        <ChoiceBox fx:id="initialConditionChoiceBox" layoutX="15.0" layoutY="127.0" prefHeight="27.0" prefWidth="117.0" />
        <Label layoutX="8.0" layoutY="96.0" text="Warunek początkowy" />
    </children>
</Pane>
